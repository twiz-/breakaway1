
<div class="page-header">
  <h2><%= @user.full_name %></h2>
  <%= link_to "Add fav", new_user_friendship_path(friend_id: @user),class: "btn btn-mini" %>
</div>  
  
  <% if @listings %>
    <% @listings.each do |listing| %>
    <div class="well span3">
      <%= youtube_thumb_embed(listing.video).html_safe %>
      <%= listing.description %>
    </div>
    <% end %>
    
  <% end %>

